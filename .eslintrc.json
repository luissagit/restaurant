{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-underscore-dangle" : "off",
        "no-unused-vars": "off",
        "no-empty-function": "off",
        "no-restricted-globals": "off",
        "require-jsdoc": "off",
        "max-len": "off",
        "new-cap": "off"
    }
}
